<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="lk.sliit.easylist">

    <application

        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
      ><meta-data
        android:name="com.google.android.gms.ads.AD_MANAGER_APP"
        android:value="true" />
        <!--<uses-permission android:name="android.permission.INTERNET"/>-->
        <!--<uses-library android:name="com.google.cloud.translate.Translate" />-->
        <activity android:name=".layout.Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".layout.SignUpBuyer"/>
        <activity android:name=".layout.SignUpAs"/>
        <activity android:name=".layout.SignUpSeller"/>
        <activity android:name=".layout.Home"/>
    </application>

</manifest>